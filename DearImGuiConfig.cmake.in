  add_library(ImGui::Backend_Metal ALIAS ImGui::imgui_impl_metal)
endif()

if(${CMAKE_FIND_PACKAGE_NAME}_FIND_REQUIRED)
  set(required REQUIRED)
endif()

if(${CMAKE_FIND_PACKAGE_NAME}_FIND_QUIETLY)
  set(quiet QUIET)
endif()

if(TARGET ImGui::Backend_Allegro5)
  find_dependency(Allegro ${quiet} ${required})
endif()

if(TARGET ImGui::Backend_GLFW)
  find_dependency(glfw3 ${quiet} ${required})
endif()

if(TARGET ImGui::Backend_FreeGLUT)
  find_dependency(FreeGLUT ${quiet} ${required})
endif()

if(TARGET ImGui::Backend_SDL2 )
  find_dependency(SDL2 ${quiet} ${required})
endif()

if(TARGET ImGui::Backend_SDLRenderer2)
  find_dependency(SDL2 ${quiet} ${required})
endif()

if(TARGET ImGui::Backend_SDL3)
  find_dependency(SDL3 ${quiet} ${required})
endif()

if(TARGET ImGui::Backend_SDLRenderer3)
  find_dependency(SDL3 ${quiet} ${required})
endif()

if(TARGET ImGui::Backend_SDLGPU3)
  find_dependency(SDL3 ${quiet} ${required})
endif()

if(TARGET ImGui::Backend_OpenGL2)
  find_dependency(OpenGL 2 ${quiet} ${required})
endif()

if(TARGET ImGui::Backend_OpenGL3)
  find_dependency(OpenGL 3 ${quiet} ${required})
endif()

if(TARGET ImGui::Backend_Vulkan)
  find_dependency(Vulkan ${quiet} ${required})
endif()

if(TARGET ImGui::Backend_WebGPU)
  find_dependency(Threads ${quiet} ${required})
  find_dependency(Dawn ${quiet} ${required})
endif()

set(is_imgui_freetype_enabled @is_imgui_freetype_enabled_in_imconfig@)
set(is_imgui_freetype_lunasvg_enabled @is_imgui_freetype_lunasvg_enabled_in_imconfig@)
set(is_imgui_freetype_plutosvg_enabled @is_imgui_freetype_plutosvg_enabled_in_imconfig@)

if(is_imgui_freetype_enabled)
  find_dependency(freetype ${quiet} ${required})

  if(is_imgui_freetype_lunasvg_enabled)
    find_dependency(lunasvg ${quiet} ${required})
  elseif(is_imgui_freetype_plutosvg_enabled)
    find_dependency(plutosvg ${quiet} ${required})
  endif()
endif()
