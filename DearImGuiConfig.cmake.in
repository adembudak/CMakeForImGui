if(TARGET ImGui::imgui_impl_opengl3)
  add_library(ImGui::Backend_OpenGL3 ALIAS ImGui::imgui_impl_opengl3)
  find_dependency(OpenGL 3 REQUIRED)
endif()

if(TARGET ImGui::imgui_impl_vulkan)
  add_library(ImGui::Backend_Vulkan ALIAS ImGui::imgui_impl_vulkan)
  find_dependency(Vulkan REQUIRED)
endif()

