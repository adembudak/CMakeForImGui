endif()

set(is_imgui_freetype_enabled @is_imgui_freetype_enabled_in_imconfig@)
set(is_imgui_freetype_lunasvg_enabled @is_imgui_freetype_lunasvg_enabled_in_imconfig@)
set(is_imgui_freetype_plutosvg_enabled @is_imgui_freetype_plutosvg_enabled_in_imconfig@)

if(is_imgui_freetype_enabled)
  find_dependency(freetype REQUIRED)

  if(is_imgui_freetype_lunasvg_enabled)
    find_dependency(lunasvg REQUIRED)
  elseif(is_imgui_freetype_plutosvg_enabled)
    find_dependency(plutosvg REQUIRED)
  endif()
endif()
