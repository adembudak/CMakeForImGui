@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

if(EXISTS "${CMAKE_CURRENT_LIST_DIR}/DearImGuiTargets.cmake")
  include("${CMAKE_CURRENT_LIST_DIR}/DearImGuiTargets.cmake")
endif()

set(_${CMAKE_FIND_PACKAGE_NAME}_supported_components imgui_test_engine imgui_club imgui_markdown ImPlot ImPlot3D ImGuiFileDialog )

set(_required_but_not_found_components)

foreach(component IN LISTS ${CMAKE_FIND_PACKAGE_NAME}_FIND_COMPONENTS)
  if(NOT component IN_LIST _${CMAKE_FIND_PACKAGE_NAME}_supported_components)
    message("Unsupported component: ${component}")
    continue()
  endif()

  if(component STREQUAL "imgui_test_engine")
    include("${CMAKE_CURRENT_LIST_DIR}/CMakeForImGui-imgui_test_engine-Targets.cmake" OPTIONAL RESULT_VARIABLE _result)
    if(${CMAKE_FIND_PACKAGE_NAME}_FIND_REQUIRED_${component} AND NOT _result)
      list(APPEND _required_but_not_found_components ${component})
    endif()
    set(${CMAKE_FIND_PACKAGE_NAME}_${component}_FOUND ${_result})
  endif()

  if(component STREQUAL "imgui_club")
    include("${CMAKE_CURRENT_LIST_DIR}/CMakeForImGui-imgui_club-Targets.cmake" OPTIONAL RESULT_VARIABLE _result)
    if(${CMAKE_FIND_PACKAGE_NAME}_FIND_REQUIRED_${component} AND NOT _result)
      list(APPEND _required_but_not_found_components ${component})
    endif()
    set(${CMAKE_FIND_PACKAGE_NAME}_${component}_FOUND ${_result})
  endif()

  if(component STREQUAL "imgui_markdown")
    include("${CMAKE_CURRENT_LIST_DIR}/CMakeForImGui-imgui_markdown-Targets.cmake" OPTIONAL RESULT_VARIABLE _result)
    if(${CMAKE_FIND_PACKAGE_NAME}_FIND_REQUIRED_${component} AND NOT _result)
      list(APPEND _required_but_not_found_components ${component})
    endif()
    set(${CMAKE_FIND_PACKAGE_NAME}_${component}_FOUND ${_result})
  endif()

  if(component STREQUAL "ImPlot")
    include("${CMAKE_CURRENT_LIST_DIR}/CMakeForImGui-ImPlot-Targets.cmake" OPTIONAL RESULT_VARIABLE _result)
    if(${CMAKE_FIND_PACKAGE_NAME}_FIND_REQUIRED_${component} AND NOT _result)
      list(APPEND _required_but_not_found_components ${component})
    endif()
    set(${CMAKE_FIND_PACKAGE_NAME}_${component}_FOUND ${_result})
  endif()

  if(component STREQUAL "ImPlot3D")
    include("${CMAKE_CURRENT_LIST_DIR}/CMakeForImGui-ImPlot3D-Targets.cmake" OPTIONAL RESULT_VARIABLE _result)
    if(${CMAKE_FIND_PACKAGE_NAME}_FIND_REQUIRED_${component} AND NOT _result)
      list(APPEND _required_but_not_found_components ${component})
    endif()
    set(${CMAKE_FIND_PACKAGE_NAME}_${component}_FOUND ${_result})
  endif()

  if(component STREQUAL "ImGuiFileDialog")
    include("${CMAKE_CURRENT_LIST_DIR}/CMakeForImGui-ImGuiFileDialog-Targets.cmake" OPTIONAL RESULT_VARIABLE _result)
    if(${CMAKE_FIND_PACKAGE_NAME}_FIND_REQUIRED_${component} AND NOT _result)
      list(APPEND _required_but_not_found_components ${component})
    endif()
    set(${CMAKE_FIND_PACKAGE_NAME}_${component}_FOUND ${_result})
  endif()
endforeach()


if(_required_but_not_found_components)
  string(JOIN " " _required_but_not_found_components_message ${_required_but_not_found_components})
  message(FATAL_ERROR "Following REQUIRED components not found: ${_required_but_not_found_components_message}")
endif()

unset(_result)
unset(_required_but_not_found_components_message)
unset(_required_but_not_found_components)

if(${CMAKE_FIND_PACKAGE_NAME}_FIND_REQUIRED)
  set(required REQUIRED)
endif()

if(${CMAKE_FIND_PACKAGE_NAME}_FIND_QUIETLY)
  set(quiet QUIET)
endif()

